GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/DFJPEGTurbo" "${PODS_ROOT}/Headers/Public/OpenCV-iOS"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/DFJPEGTurbo" $(inherited) "${PODS_ROOT}/DFJPEGTurbo/DFJPEGTurbo/libturbojpeg" $(inherited) "${PODS_ROOT}/OpenCV-iOS/install/lib"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/DFJPEGTurbo" -isystem "${PODS_ROOT}/Headers/Public/OpenCV-iOS"
OTHER_LDFLAGS = $(inherited) -ObjC -l"DFJPEGTurbo" -l"opencv2" -l"stdc++" -l"turbojpeg" -framework "AVFoundation" -framework "Accelerate" -framework "AssetsLibrary" -framework "CoreGraphics" -framework "CoreImage" -framework "CoreMedia" -framework "CoreVideo" -framework "Foundation" -framework "QuartzCore" -framework "UIKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
